{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ProductList from './components/ProductList.vue';\nimport ShoppingCart from './components/ShoppingCart.vue';\nexport default {\n  components: {\n    ProductList,\n    ShoppingCart\n  },\n  data() {\n    return {\n      products: [{\n        id: 1,\n        name: 'Sling Bag',\n        price: 50\n      }, {\n        id: 2,\n        name: 'Tote Bag',\n        price: 70\n      }, {\n        id: 3,\n        name: 'Leather Bagpack',\n        price: 90\n      }],\n      cart: []\n    };\n  },\n  computed: {\n    totalPrice() {\n      return this.cart.reduce((total, item) => total + item.product.price * item.quantity, 0);\n    }\n  },\n  methods: {\n    addToCart(product) {\n      const foundItem = this.cart.find(item => item.product.id === product.id);\n      if (foundItem) {\n        foundItem.quantity++;\n      } else {\n        this.cart.push({\n          product: product,\n          quantity: 1\n        });\n      }\n    },\n    removeFromCart(item) {\n      const index = this.cart.indexOf(item);\n      if (index !== -1) {\n        this.cart.splice(index, 1);\n      }\n    },\n    updateQuantity(payload) {\n      const foundItem = this.cart.find(item => item.product.id === payload.id);\n      if (foundItem) {\n        foundItem.quantity = parseInt(payload.quantity);\n      }\n    }\n  }\n};","map":{"version":3,"names":["ProductList","ShoppingCart","components","data","products","id","name","price","cart","computed","totalPrice","reduce","total","item","product","quantity","methods","addToCart","foundItem","find","push","removeFromCart","index","indexOf","splice","updateQuantity","payload","parseInt"],"sources":["C:\\Users\\pbana\\activity-3\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <ProductList :products=\"products\" @add-to-cart=\"addToCart\" />\n    <ShoppingCart :cart=\"cart\" @remove-from-cart=\"removeFromCart\" @update-quantity=\"updateQuantity\" />\n    <p>Total Price: â‚±{{ totalPrice }}</p>\n  </div>\n</template>\n\n<script>\nimport ProductList from './components/ProductList.vue';\nimport ShoppingCart from './components/ShoppingCart.vue';\n\nexport default {\n  components: {\n    ProductList,\n    ShoppingCart\n  },\n  data() {\n    return {\n      products: [\n        { id: 1, name: 'Sling Bag', price: 50 }, \n        { id: 2, name: 'Tote Bag', price: 70, },\n        { id: 3, name: 'Leather Bagpack', price: 90, }\n      ],\n      cart: []\n    }\n  },\n  computed: {\n    totalPrice() {\n      return this.cart.reduce((total, item) => total + (item.product.price * item.quantity), 0);\n    }\n  },\n  methods: {\n    addToCart(product) {\n      const foundItem = this.cart.find(item => item.product.id === product.id);\n      if (foundItem) {\n        foundItem.quantity++;\n      } else {\n        this.cart.push({ product: product, quantity: 1 });\n      }\n    },\n    removeFromCart(item) {\n      const index = this.cart.indexOf(item);\n      if (index !== -1) {\n        this.cart.splice(index, 1);\n      }\n    },\n    updateQuantity(payload) {\n      const foundItem = this.cart.find(item => item.product.id === payload.id);\n      if (foundItem) {\n        foundItem.quantity = parseInt(payload.quantity);\n      }\n    }\n  }\n}\n</script>"],"mappings":";AASA,OAAOA,WAAU,MAAO,8BAA8B;AACtD,OAAOC,YAAW,MAAO,+BAA+B;AAExD,eAAe;EACbC,UAAU,EAAE;IACVF,WAAW;IACXC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,CACR;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,WAAW;QAAEC,KAAK,EAAE;MAAG,CAAC,EACvC;QAAEF,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,UAAU;QAAEC,KAAK,EAAE;MAAI,CAAC,EACvC;QAAEF,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,iBAAiB;QAAEC,KAAK,EAAE;MAAI,EAC9C;MACDC,IAAI,EAAE;IACR;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAO,IAAI,CAACF,IAAI,CAACG,MAAM,CAAC,CAACC,KAAK,EAAEC,IAAI,KAAKD,KAAI,GAAKC,IAAI,CAACC,OAAO,CAACP,KAAI,GAAIM,IAAI,CAACE,QAAS,EAAE,CAAC,CAAC;IAC3F;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAACH,OAAO,EAAE;MACjB,MAAMI,SAAQ,GAAI,IAAI,C